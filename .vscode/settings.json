// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": true,
  "editor.wrappingColumn": 120,

  "diffEditor.ignoreTrimWhitespace": false,

  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.vscode": true
  },

  "javascript.validate.enable": false
}